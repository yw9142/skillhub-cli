{
  "name": "@yw9142/skillhub-cli",
  "version": "0.3.0",
  "description": "SkillHub CLI - sync skills with GitHub Gist",
  "bin": {
    "skillhub": "bin/skillhub.js"
  },
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc && tsc-alias",
    "prepare": "npm run build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish --access public"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yw9142/skillhub-cli.git"
  },
  "keywords": [
    "cli",
    "skills",
    "agent",
    "gist",
    "sync"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "commander": "^11.1.0",
    "conf": "^12.0.0",
    "inquirer": "^9.2.12"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.10",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3",
    "vitest": "^2.1.8"
  }
}
